{
    "tasks": [
        {
            "label": "ReleaseBuild",
            "type": "shell",
            "command": "cmake --build build --config Release",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "ReleaseWindeployqt",
            "type": "shell",
            "command": "windeployqt .\\build\\Release\\CUITExamClient.exe",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": "ReleaseBuild"
        },
        {
            "label": "preparePacking",
            "type": "shell",
            "command": ".\\preparePacking.bat",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": "ReleaseWindeployqt"
        }
    ],
    "version": "2.0.0"
}